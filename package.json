{
  "name": "ci-reports",
  "description": "reports for ci",
  "version": "0.0.1",
  "author": "Heitor Araujo",
  "bugs": "https://github.com/hsaraujo/ci-reports/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/errors": "^1.3.5",
    "@salesforce/command": "^4.1.2",
    "@salesforce/core": "^2.28.0",
    "@salesforce/plugin-apex": "^0.2.9",
    "@salesforce/plugin-source": "^1.1.1",
    "@types/shelljs": "^0.8.9",
    "@types/xml2js": "^0.4.9",
    "shelljs": "^0.8.4",
    "tslib": "2.3.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/plugin-help": "^3.2.3",
    "@oclif/test": "^1.2.8",
    "@salesforce/dev-config": "1.4.1",
    "@types/chai": "^4.2.22",
    "@types/jsforce": "^1.9.35",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "globby": "^12.0.2",
    "mocha": "^9.1.2",
    "nyc": "^15.1.0",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/hsaraujo/ci-reports",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "hello": {
        "description": "Commands to say hello."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "hsaraujo/ci-reports",
  "scripts": {
    "lint": "tslint --project . --config tslint.json --format stylish",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
