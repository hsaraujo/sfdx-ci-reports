{
  "name": "sfdx-ci-reports",
  "description": "Reports for Salesforce CI/CD",
  "version": "0.0.1",
  "author": "Heitor Araujo",
  "bugs": "https://github.com/hsaraujo/sfdx-ci-reports/issues",
  "dependencies": {
    "@oclif/config": "1.17.0",
    "@oclif/errors": "1.3.5",
    "@oclif/plugin-help": "3.2.2",
    "@salesforce/command": "4.0.4",
    "@types/shelljs": "^0.8.9",
    "@types/sinonjs__fake-timers": "^6.0.4",
    "@types/xml2js": "^0.4.9",
    "npm-check-updates": "11.8.3",
    "shelljs": "^0.8.4",
    "tslib": "2.3.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.26.0",
    "@oclif/test": "1.2.8",
    "@salesforce/dev-config": "2.1.2",
    "@types/chai": "4.2.21",
    "@types/jsforce": "1.9.33",
    "@types/mocha": "9.0.0",
    "@types/node": "16.4.13",
    "globby": "11.0.1",
    "mocha": "9.0.3",
    "nyc": "15.1.0",
    "sinon": "9.2.1",
    "ts-node": "10.2.0",
    "typescript": "4.3.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/hsaraujo/sfdx-ci-reports",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "report": {
        "description": "Report"
      },
      "coverage": {
        "description": "Coverage"
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "hsaraujo/sfdx-ci-reports",
  "scripts": {
    "lint": "tslint --project . --config tslint.json --format stylish",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
